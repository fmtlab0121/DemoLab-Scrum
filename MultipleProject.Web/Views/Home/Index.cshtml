@model List<Customer>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to DemoLab-Scrum - Features 2 - change</h1>
    <p>@ViewBag.Comment</p>

</div>

<div>
    @for (int i = 0; i < Model.Count; i++)
    {
        <p>ID : @Model[i].Id</p>
        <p>Name : @Model[i].Name</p>
        <hr />
    }
</div>